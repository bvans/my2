<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>transfer</servlet-name>
        <servlet-class>servlets.Transfer</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Guide_csa_py</servlet-name>
        <servlet-class>servlets.Guide_csa_py</servlet-class>
    </servlet>

    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>Csa_sprite_py</servlet-name>
        <servlet-class>servlets.Csa_sprite_py</servlet-class>
    </servlet>

    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>Ask</servlet-name>
        <servlet-class>servlets.Ask</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>AuthorRization</servlet-name>
        <servlet-class>servlets.AuthorRization</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>CoreServlet</servlet-name>
        <servlet-class>servlets.CoreServlet</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>Test</servlet-name>
        <servlet-class>servlets.Test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>transfer</servlet-name>
        <url-pattern>/servlet/Transfer</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Guide_csa_py</servlet-name>
        <url-pattern>/servlet/Guide_csa_py</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Csa_sprite_py</servlet-name>
        <url-pattern>/servlet/csa_sprite_py</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Ask</servlet-name>
        <url-pattern>/servlet/ask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthorRization</servlet-name>
        <url-pattern>/servlet/AuthorRization</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CoreServlet</servlet-name>
        <url-pattern>/servlet/core</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/servlet/Test</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>/index.htm</welcome-file>
    </welcome-file-list>

</web-app>
